    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Server",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "application",
                "FLASK_ENV": "development",
                "FLASK_RUN_PORT": "4000",
                "SYLLABITS_MODE": "development",
                "SYLLABITS_SECRET_KEY": "thisisadummykeyfordevelopment"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "justMyCode": false
        },
        {
            "name": "Syllabits Server (Shell)",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "application",
                "FLASK_ENV": "development",
                "FLASK_RUN_PORT": "4000",
                "SYLLABITS_MODE": "development",
                "SYLLABITS_SECRET_KEY": "thisisadummykeyfordevelopment"
            },
            "args": [
                "shell"
            ],
            "justMyCode": false
        },
        {
            "name": "Schema Testing",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "testing",
                "FLASK_ENV": "development",
            },
            "args": [
